pipeline {
    agent any
    stages {
        stage('Build') { 
            steps {
                sh 'mvn -B -DskipTests clean install' 
            }
        }
        stage('Unit Test') { 
            steps {
                sh 'mvn clean test'  
            }
        }
        stage('Run') { 
            steps {
                sh 'mvn clean package'  
            }
        }             
    }
}
